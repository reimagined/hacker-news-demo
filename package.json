{
  "name": "hacker-news-resolve",
  "version": "1.0.0",
  "description": "A React & Redux & Resolve implementation of Hacker News",
  "scripts": {
    "build": "resolve-scripts build",
    "start": "resolve-scripts start",
    "dev": "resolve-scripts dev",
    "import": "babel-node ./import ",
    "prettier": "prettier --single-quote --no-semi --write \"**/*.js\"",
    "test": "jest",
    "test:coverage": "jest --u --coverage",
    "test:watch": "jest --watchAll",
    "lint-staged": "lint-staged"
  },
  "pre-commit": [
    "lint-staged",
    "test"
  ],
  "lint-staged": {
    "*.{js,css}": [
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "keywords": [
    "react",
    "redux",
    "resolve",
    "hackernews",
    "event-sourcing",
    "cqrs"
  ],
  "author": "reimagined team",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "js-cookie": "^2.1.4",
    "jsonwebtoken": "^7.4.3",
    "nedb": "^1.8.0",
    "plur": "^2.1.2",
    "progress": "^2.0.0",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-pure-render": "^1.0.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-timeago": "^3.4.3",
    "redux": "^3.7.2",
    "redux-saga": "^0.14.8",
    "resolve-bus-memory": "0.0.28",
    "resolve-es": "0.0.28",
    "resolve-redux": "0.0.28",
    "resolve-scripts": "0.0.28",
    "resolve-storage-file": "0.0.28",
    "sanitizer": "^0.1.3",
    "seamless-immutable": "^7.1.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "flow-bin": "^0.52.0",
    "jest": "^20.0.4",
    "lint-staged": "4.0.4",
    "pre-commit": "^1.2.2",
    "prettier": "1.6.0",
    "react-test-renderer": "^15.6.1",
    "sinon": "^3.2.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/coverage/",
      "resolve.build.config.js",
      "resolve.client.config.js",
      "resolve.server.config.js",
      "common/immutable.js",
      "server/"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less)$": "<rootDir>/tests/ignore-css.js"
    },
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  }
}
